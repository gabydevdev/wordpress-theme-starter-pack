{
  "name": "wptsp",
  "version": "1.0.0",
  "description": "WordPress Theme Starter Pack",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"webpack --watch --mode=development\" \"sass --watch assets/scss:assets/css\"",
    "build": "webpack --mode=production && sass assets/scss:assets/css --style=compressed",
    "lint:js": "eslint assets/js/src/**/*.js",
    "lint:scss": "stylelint assets/scss/**/*.scss",
    "format": "prettier --write \"**/*.{js,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,scss,json,md}\""
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@wordpress/eslint-plugin": "^17.10.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3"
  }
}
